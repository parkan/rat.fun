{
  "name": "rat-room",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/moving-castles/rat-room.git"
  },
  "scripts": {
    "build": "pnpm recursive run build",
    "build:client": "pnpm --filter 'client' run build",
    "dev": "mprocs",
    "dev:client": "pnpm --filter 'client' run dev",
    "dev:contracts": "pnpm --filter 'contracts' run dev",
    "foundry:up": "curl -L https://foundry.paradigm.xyz | bash && bash $HOME/.foundry/bin/foundryup",
    "mud:up": "pnpm mud set-version --tag main && pnpm install",
    "prepare": "(forge --version || pnpm foundry:up)",
    "test": "pnpm recursive run test",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "devDependencies": {
    "@latticexyz/cli": "2.2.23-ba07cf09273fba0a246eaf341cdb4eec5328f120",
    "@latticexyz/common": "2.2.23-ba07cf09273fba0a246eaf341cdb4eec5328f120",
    "@latticexyz/explorer": "2.2.23-ba07cf09273fba0a246eaf341cdb4eec5328f120",
    "@latticexyz/store-indexer": "2.2.23-ba07cf09273fba0a246eaf341cdb4eec5328f120",
    "@typescript-eslint/eslint-plugin": "8.36.0",
    "@typescript-eslint/parser": "8.36.0",
    "eslint": "9.30.1",
    "mprocs": "^0.7.3",
    "prettier": "^3.6.2",
    "prettier-plugin-solidity": "^2.0.0",
    "prettier-plugin-svelte": "^3.4.0",
    "shx": "^0.4.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": "^20",
    "pnpm": "^10"
  },
  "resolutions": {
    "brace-expansion": "2.0.1"
  },
  "packageManager": "pnpm@10.8.0+sha512.0e82714d1b5b43c74610193cb20734897c1d00de89d0e18420aebc5977fa13d780a9cb05734624e81ebd81cc876cd464794850641c48b9544326b5622ca29971",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ],
    "overrides": {
      "wagmi": "2.15.6",
      "@tanstack/react-query": "5.81.5"
    }
  }
}
