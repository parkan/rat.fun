{
  "name": "rat.fun",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Moving-Castles/rat.fun.git"
  },
  "scripts": {
    "build": "pnpm recursive run build",
    "build:client": "pnpm --filter 'client' run build",
    "dev": "mprocs",
    "dev:client": "pnpm --filter 'client' run dev",
    "dev:contracts": "pnpm --filter 'contracts' run dev",
    "foundry:up": "curl -L https://foundry.paradigm.xyz | bash && bash $HOME/.foundry/bin/foundryup",
    "mud:up": "pnpm mud set-version --tag main && pnpm install",
    "prepare": "(forge --version || pnpm foundry:up) && husky install",
    "test": "pnpm recursive run test",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "pre-commit": "node scripts/pre-commit/check-credentials.js && node scripts/pre-commit/check-env-files.js"
  },
  "devDependencies": {
    "@latticexyz/cli": "2.2.23-94cac741be24bb5743f2f36235cc3bb40012417a",
    "@latticexyz/common": "2.2.23-94cac741be24bb5743f2f36235cc3bb40012417a",
    "@latticexyz/explorer": "2.2.23-94cac741be24bb5743f2f36235cc3bb40012417a",
    "@latticexyz/store-indexer": "2.2.23-94cac741be24bb5743f2f36235cc3bb40012417a",
    "@typescript-eslint/eslint-plugin": "8.46.2",
    "@typescript-eslint/parser": "8.46.2",
    "eslint": "9.38.0",
    "husky": "^9.1.7",
    "mprocs": "^0.7.3",
    "prettier": "^3.6.2",
    "prettier-plugin-solidity": "^2.1.0",
    "prettier-plugin-svelte": "^3.4.0",
    "shx": "^0.4.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": "^20",
    "pnpm": "^10"
  },
  "packageManager": "pnpm@10.18.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
