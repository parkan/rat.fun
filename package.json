{
  "name": "rat-room",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/moving-castles/rat-room.git"
  },
  "scripts": {
    "build": "pnpm recursive run build",
    "build:client": "pnpm --filter 'client' run build",
    "dev": "mprocs",
    "dev:client": "pnpm --filter 'client' run dev",
    "dev:contracts": "pnpm --filter 'contracts' run dev",
    "foundry:up": "curl -L https://foundry.paradigm.xyz | bash && bash $HOME/.foundry/bin/foundryup",
    "mud:up": "pnpm mud set-version --tag main && pnpm install",
    "prepare": "(forge --version || pnpm foundry:up)",
    "test": "pnpm recursive run test"
  },
  "devDependencies": {
    "@latticexyz/cli": "2.2.22-fc522a899c932f53675e624805e97a4eeefca9e8",
    "@latticexyz/common": "2.2.22-fc522a899c932f53675e624805e97a4eeefca9e8",
    "@latticexyz/explorer": "2.2.22-fc522a899c932f53675e624805e97a4eeefca9e8",
    "@latticexyz/store-indexer": "2.2.22-fc522a899c932f53675e624805e97a4eeefca9e8",
    "@typescript-eslint/eslint-plugin": "8.34.0",
    "@typescript-eslint/parser": "8.34.0",
    "eslint": "9.28.0",
    "mprocs": "^0.7.3",
    "shx": "^0.4.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": "^20",
    "pnpm": "^10"
  },
  "resolutions": {
    "brace-expansion": "2.0.1"
  },
  "packageManager": "pnpm@10.8.0+sha512.0e82714d1b5b43c74610193cb20734897c1d00de89d0e18420aebc5977fa13d780a9cb05734624e81ebd81cc876cd464794850641c48b9544326b5622ca29971",
  "pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	}
}
